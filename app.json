{
  "expo": {
    "scheme": "acme",
    "name": "Drugdex",
    "slug": "Drugdex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#ffffff",
        "resizeMode": "cover"
      },
      "package": "com.example.Drugdex"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "37f79a52-0a94-45dd-b716-bf889820558f"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
